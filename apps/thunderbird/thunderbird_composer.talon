app: thunderbird_composer
-
# mail
(draft | mail | message) save: key("{user.mod()}-s")
(draft | mail | message) print: key("{user.mod()}-p")
(draft | mail | message) send: key("{user.mod()}-enter")
# layout
toggle contacts: key(f9)
# navigation
go (inbox | thunderbird | main): key("{user.mod()}-1")
# edit
cite paste: key("{user.mod()}-shift-o")
(unformatted | raw) paste: key("{user.mod()}-shift-v")
link new: key("{user.mod()}-k")
link delete: key("{user.mod()}-shift-k")
