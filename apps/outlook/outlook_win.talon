os: windows
and app: Outlook
os: windows
and app.name: Microsoft Outlook
os: windows
and app.exe: OUTLOOK.EXE

-
archive: key(alt h o 1)
new e-mail: key(ctrl-n)
calendar: key(ctrl-2)
inbox: key(ctrl-1)
Reply: key(ctrl-r)
Reply all: key(ctrl-shift-r)
Forward: key(ctrl-f)
accept: key(shift-f10 c c enter)
go:
    key(ctrl-q)
    sleep(100ms)
    key(down)
unread: key(ctrl-u)
flag: key(insert)